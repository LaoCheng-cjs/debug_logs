<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>文档说明</title>
    <link rel="stylesheet" href="<%= url %>/css/index.css">
    <link rel="stylesheet" href="<%= url %>/css/see.css">
</head>
<body>
    <div class="see-wrap">
        <div class="nav-wrap">
            <div class="nav-list active">全部</div>
            <div class="nav-list">安卓</div>
            <div class="nav-list">IOS</div>
            <div class="nav-list">PC</div>
        </div>
        <div class="nav-content">
            <div id="container" style="height: 400px"></div>
            <!-- <input type="text" id="calendar"> -->
        </div>
        <!--  -->
    </div>
    <script type="text/javascript" src="<%= url %>/js/jquery.min.js"></script>
    <!-- 可视化 -->
    <script src="<%= url %>/js/echarts.min.js"></script>
    <script src="<%= url %>/js/china.js"></script>
    <!-- china.js -->
    <script>
        if (self == top) { 
            location.href = '/';
        }
        $(function () {
            var dom = document.getElementById("container");
            var myChart = echarts.init(dom); 
            var app = {};
            var option = null;
            option = {
                color: ['#3398DB'],
                tooltip: {
                    trigger: 'axis',
                    axisPointer: {            // 坐标轴指示器，坐标轴触发有效
                        type: 'shadow'        // 默认为直线，可选为：'line' | 'shadow'
                    }
                },
                grid: {
                    left: '3%',
                    right: '4%',
                    bottom: '3%',
                    containLabel: true
                },
                xAxis: [
                    {
                        type: 'category',
                        data: ['2019-12-02', '2019-12-03', '2019-12-04', '2019-12-05', '2019-12-06', '2019-12-07',
                        '2019-12-08','2019-12-09','2019-12-10'],
                        axisTick: {
                            alignWithLabel: true
                        }
                    }
                ],
                yAxis: [
                    {
                        type: 'value'
                    }
                ],
                series: [
                    {
                        name: '直接访问',
                        type: 'bar',
                        barWidth: '60%',
                        data: [10, 52, 200, 334, 390, 330, 220]
                    }
                ]
            };
            if (option && typeof option === "object") {
                myChart.setOption(option, true);
            }
        })
    </script>
</body>
</html>