    <!-- 可视化 -->
    <script src="<%= url %>/js/echarts.min.js"></script>
    <script src="<%= url %>/js/javascript.min.js"></script>
    <script src="<%= url %>/js/clipboard.min.js"></script>
    <script>
        $(function () {
            // 初始化代码高亮
            hljs.initHighlighting();
            $("code").each(function () {
                $(this).html("<ol><li>" + $(this).html().replace(/\n/g, "\n</li><li>") + "\n</li></ol>");
            });
            $('.minimize').click(function () {
                $(this).parent().siblings('pre').stop().animate({height: 'toggle'})
            })
            $('.minimize').eq(0).parent().siblings('pre').delay(500).animate({height: 'toggle'})
            // 绑定复制按钮
            var clipboard = new ClipboardJS('.copy',{
                text: function(trigger) {
                   return $(trigger).siblings('pre').text()
                }
            });
            clipboard.on('success', function(e) {
                $('.toast').text('复制成功').show()
                time()
                e.clearSelection();
            });
            clipboard.on('error', function(e) {
                $('.toast').text('复制失败').show()
                time()
                console.error('Action:', e.action);
                console.error('Trigger:', e.trigger);
            });
            let timer = null;
            function time () {
                clearTimeout(timer)
                timer = setTimeout(() => {
                    $('.toast').hide()
                },2000)
            }
